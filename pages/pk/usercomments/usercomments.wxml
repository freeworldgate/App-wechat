
<view class="css z3 b-white"  style="min-height:100vh">

  <view class="css" >
      <view class="rcs " style="height:{{top}}px"></view>
      <view class="rcs " style="height:32px"></view>
      <view class="rcs " style="height:{{5}}px"></view>
  </view>

  <view wx:if="{{!comments || comments.length>0}}" class="css z3">
      <view class="h-10 b-48"></view>
      <view class="css" wx:for="{{comments}}" wx:for-index="bindex" wx:for-item="comment" wx:key="commentkey">
          <view class="css b-line">
              <view class="css p-5vw">
                  <view class="rbc">
                      <!-- <view class="rcc" bindtap="userPage" data-user="{{comment.user}}"><image class="rcc box8 " style="width:10vw;height:10vw;border-radius:{{borderRadius}}vw" src="{{comment.user.imgUrl}}" mode="aspectFill"></image></view> -->
                      <!-- <view class="w-20"></view> -->
                      <view class="csc" style="flex:1" bindtap="userPage" data-user="{{comment.user}}">
                          <!-- <view class="rus"><view class="line-1-center fs-25 fw-610 c-username">{{comment.user.userName}}</view></view> -->
                          <view class="rus"><view class="rcc fs-25 fw-300 c-4">{{comment.time}}</view></view>
                      </view>
                      <view class="rcc" style="width:10vw" bindtap="oper" data-index="{{bindex}}" data-commentid="{{comment.commentId}}" data-post="{{comment.postColum}}"><image class="rcc"  style="width:5vw;height:5vw"  src="/images/oper.png" mode="aspectFit"></image></view>
                  </view>
                  <view class="rss">
                      <!-- <view class="rss"><view class="w-10vw"></view><view class="w-20"></view></view> -->
                      <view class="css" style="flex:1">
                          <view class="css pt-10 pb-10">
                              <text class="line-5-start rus fs-33 line-h43 fw-400 "  bindtap="showText" data-text="{{comment.comment}}" >{{comment.comment}}</text>
                          </view>
                          <view class="css p-20 b-49">
                              <text class="line-1-start rus fs-30 fw-700"   bindtap="inPk" data-pkid="{{comment.postColum.pkId}}">{{comment.postColum.pkName}}</text>
                              <view class="h-10"></view>
                              <view class="rss" bindtap="comment" data-post="{{comment.postColum}}">
                                  <view class="css" style="flex:1">
                                    <text class="line-2-start rus fs-30 line-h40 fw-400" >{{comment.postColum.text}}</text>
                                  </view>
                                  <view class="w-20"></view>
                                  <view class="rcc"> 
                                      <!-- <image class="rcc w-90 h-90" src="{{comment.postColum.postType===3?comment.postColum.contentUrl:'/images/noimg.png'}}" mode="aspectFill"></image> -->
                                        <view wx:if="{{comment.postColum.postType===4}}" class="rcc">
                                            <video class="w-120 h-120" src="{{comment.postColum.contentUrl}}" objectFit="cover" controls="{{false}}" play-btn-position="center" coustom-cache="{{false}}" show-center-play-btn="{{false}}"></video>
                                            <view class="rcc absolute w-120 h-120 b-post"> <image class="rcc w-60 h-60" src="/images/video.png" mode="aspectFit"></image></view>
                                        </view>
                                        <image wx:elif="{{comment.postColum.postType===3}}" class="rcc w-120 h-120" src="{{comment.postColum.contentUrl}}" mode="aspectFill"></image>
                                        <image wx:else class="rcc w-120 h-120" src="/images/noimg.png" mode="aspectFill"></image>
                                  </view>
                              </view>
                          </view>
                          <view class="rbc-nowrap pt-30">
                                                                      <view class="rcc">
                                                                      </view>
                                                                  
                                                                      <view class="rcs">
                                                                            <view class="rcs w-130 h-35"  >
                                                                                <view class="rcs" >
                                                                                    <image class="rcc w-30 h-30" src="/images/like1.png" mode="aspectFit"></image>
                                                                                    <view class="w-20"></view>
                                                                                    <view class="fs-25 fw-300">{{comment.likes}}</view>
                                                                                </view>
                                                                            </view>
                                                                            <view class="rcs w-130 h-35">
                                                                                <view class="rcs" >
                                                                                    <image class="rcc w-30 h-30" src="/images/dislike1.png" mode="aspectFit"></image>
                                                                                    <view class="w-20"></view>
                                                                                    <view class="fs-25 fw-300">{{comment.dislikes}}</view>
                                                                                </view>
                                                                            </view>
                                                                            <view class="rcs  h-35" bindtap="restore" data-comment="{{comment}}">
                                                                                <image class="rcc w-30 h-30" src="/images/comment.png" mode="aspectFit"></image>
                                                                                <view class="w-20"></view>
                                                                                <view  class="fs-25 fw-300">{{comment.restores}}</view>
                                                                            </view>

                                                                      </view>
                          </view>
                      </view>








                  </view>
    
  

              </view>
  
          </view>
      </view>
      <view class="css h-300">
                <view hidden="{{!nomore}}" class="rcc h-100 fs-30 fw-400 c-3">没有更多...</view>
      </view>
  </view> 
  <view wx:else class="ccc" style="flex:1">
      <view class="rcc">
          <image class="h-300 w-300" mode="aspectFit" src="/images/emptydata.png" ></image>
      </view>
      <view class="fs-30 fw-400 c-3">没有评论信息</view>
  </view>









</view>






<!-- //头部 -->
<view class="css scrollCustomBar z3 box8" style="background-color:#ffffff">
<view class="css pl-4vw pr-4vw" >
<view class="rcs " style="height:{{top}}px"></view>
<view class="rcs "  bindtap="back" style="height:32px">
    <view class="rcc h-32px w-50">
        <image class="h-30 w-30" mode="aspectFit" src="/images/back.png"></image>
    </view>
    <view class="w-50"></view>
    <view class="rus w-400"><view class="rus fs-35 fw-400 line-1-start">我的评论 ({{commentNums}})</view></view>
   
</view>
<view class="rcs " style="height:{{5}}px"></view>
</view>
</view>

















